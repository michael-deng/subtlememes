<h1>Subtle Memes</h1>

<div>
    <%= link_to 'Submit new meme', new_meme_path %>
    <%= link_to 'Sign up', new_user_path %>
    <%= link_to 'Add new tag', new_tag_path %>
</div>

<% @memes.each do |meme| %>
    <%= link_to meme_path(meme) do %>
        <img src=<%= meme.link %> />
    <% end %>
<% end %>
